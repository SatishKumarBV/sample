pipeline{
	agent any
	tools{
		maven 'maven-3.6.3'
	}
	stages {
		stage('checkout scm'){
			steps{
				git branch: 'main',
					url: 'https://github.com/SatishKumarBV/sample.git'
			}
		}
		stage('build'){
			steps{
				sh 'mvn clean package'
			}
	    }
		
		stage('test'){
			steps{
				sh 'mvn test'
			}
		}
    }
}
